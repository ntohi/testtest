<!DOCTYPE html><$mt:Include module="config"$>
<html lang="<$mt:BlogLanguage$>" itemscope itemtype="http://schema.org/WebPage">
<head>
<meta charset="<$mt:PublishCharset$>">
<title><$mt:ArchiveTitle$> | <$mt:BlogName encode_html="1"$></title>
<meta name="description" content="<$mt:CategoryLabel encode_html="1"$>カテゴリのニュース">
<meta name="keywords" content="<$mt:GetVar name="metakeywords"$>,<$mt:CategoryLabel encode_html="1"$>">
<meta name="viewport" content="width=device-width">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<link rel="shortcut icon" href="<$mt:BlogURL encode_html="1"$>images/favicon.ico">
<link rel="start" href="<$mt:BlogURL encode_html="1"$>">
<link rel="alternate" type="application/atom+xml" title="Recent Entries" href="<$mt:Link template="feed_recent"$>">
<$mt:CanonicalLink$>

<!-- Open Graph Protocol -->
<meta property="og:type" content="article">
<meta property="og:locale" content="<$mt:BlogLanguage setvar="blog_lang"$><mt:If name="blog_lang" eq="ja">ja_JP<mt:Else><$mt:Var name="blog_lang"$></mt:If>">
<meta property="og:title" content="<$mt:CategoryLabel encode_html="1"$> | <$mt:BlogName encode_html="1"$>">
<meta property="og:url" content="<$mt:CategoryArchiveLink encode_html="1"$>">
<meta property="og:description" content="<$mt:CategoryLabel encode_html="1"$>カテゴリのニュース">
<meta property="og:site_name" content="<$mt:BlogName encode_html="1"$>">
<meta property="og:image" content="<$mt:BlogURL encode_html="1"$>images/ogimage.png">
<!-- Microdata -->
<meta itemprop="description" content="<$mt:CategoryLabel encode_html="1"$>カテゴリのニュース">
<link itemprop="url" href="<$mt:CategoryArchiveLink encode_html="1"$>">
<link itemprop="image" href="<$mt:BlogURL encode_html="1"$>images/ogimage.png">
<$mt:Include module="header-script"$>
</head>
<body class="category-entry-listing">

<div class="off-canvas-wrap">
<div class="inner-wrap">

<$mt:Include module="header-menu"$>

<div class="row">
  <div class="columns medium-12">
    <nav role="navigation">
      <ul class="breadcrumbs">
        <li class="home"><a href="<$mt:BlogRelativeURL$>"><i class="fa fa-home fa-lg"></i></a></li>
        <li><a href="<$mt:BlogRelativeURL$>news.html"><span>ニュース</span></a></li>
        <li><span><$mt:CategoryLabel encode_html="1"$></span></li>
      </ul>
    </nav>
   </div>
 </div>

<div class="row">
  <div class="columns medium-12">
    <div id="mainvisual">
      <img src="<$mt:BlogURL encode_html="1"$>images/slide01.jpg" alt="" width="0" height="0">
      <div class="page-banner">
        <div class="inner">
          <h2><i class="fa fa-sun-o"></i> <a href="<$mt:BlogRelativeURL$>news.html">ニュース</a></h2>
        </div>
      </div>
    </div><!-- mainvisual -->
  </div><!-- columns -->
</div><!-- row -->

<div class="row">
  <div class="columns medium-8 large-8">
    <div id="content">
      <h3 class="page-title"><$mt:CategoryLabel encode_html="1"$>のニュース一覧</h3>
      <div id="entry-list">
        <$mt:Include module="entry-list"$>
      </div>
    </div>
  </div><!-- columns -->
  <div class="columns medium-4 large-4">
    <$mt:Include module="news-sidebar"$>
  </div><!-- columns -->
</div><!-- row -->

<$mt:Include module="footer"$>

<$mt:Include module="left-menu"$>

</div><!-- inner-wrap -->
</div><!-- off-canvas-wrap -->

<$mt:Include module="footer-script"$>
</body>
</html>
